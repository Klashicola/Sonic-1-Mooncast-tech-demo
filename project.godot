config_version=5

[application]

config/name="MoonCast Engine"
config/description="A 2D and 3D Sonic physics engine for Godot 4"
config/version="0.4"
run/main_scene="res://Demo/scenes/gui/main_menu.tscn"
config/quit_on_go_back=false
config/features=PackedStringArray("4.4", "GL Compatibility")
boot_splash/bg_color=Color(0, 0.734321, 0.70697, 1)
boot_splash/image="uid://bfu7a4kbkc0up"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="uid://rjo8bqqupdua"

[display]

window/size/viewport_width=420
window/size/viewport_height=240
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/stretch/scale_mode="integer"
display_server/driver.linuxbsd="x11"

[editor]

movie_writer/movie_file="res://showcase.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/mooncast_docs/plugin.cfg")

[filesystem]

import/blender/enabled=false

