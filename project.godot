; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="MoonCast Engine"
config/description="A 2D and 3D Sonic physics engine for Godot 4"
config/version="0.1"
run/main_scene="res://Demo/main_menu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(1, 0.457673, 0, 1)
boot_splash/image="res://splash.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://icon.svg"

[display]

window/size/viewport_width=420
window/size/viewport_height=224
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_width"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
